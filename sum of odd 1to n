//Write a function to print the sum of all odd numbers from 1 to n.


import java.util.*;
class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int n =  sc.nextInt();
        int sum = 0;
        for(int i =1; i<=n; i=i+2 ){
            sum = sum+ i;
            System.out.println(sum);
        }
        System.out.println(sum);

    }
}
